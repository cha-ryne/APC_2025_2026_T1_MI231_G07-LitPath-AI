# Django Backend Environment Variables
DJANGO_SECRET_KEY=your-secret-key-here
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1

# Gemini API Key
# Get from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your-gemini-api-key-here

# Supabase Database Configuration
# Get from: Supabase Dashboard > Project Settings > Database > Connection String
# IMPORTANT: Use Transaction Pooler (port 6543) for better connection handling
DATABASE_URL=postgresql://postgres.PROJECT_ID:YOUR_PASSWORD@aws-X-region.pooler.supabase.com:6543/postgres
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here
